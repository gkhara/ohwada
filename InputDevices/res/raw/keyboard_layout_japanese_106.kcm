#
# keyboard layout Japanese 106
# 

type OVERLAY

# added
map key 26  AT
map key 27  LEFT_BRACKET
map key 43  RIGHT_BRACKET

### ROW 1

# ZENKAKU_HANKAKU etc are defined in KeycodeLabels.h
key GRAVE {
    base:                             fallback ZENKAKU_HANKAKU
    alt:                            	fallback SWITCH_CHARSET
}

key 1 {
    label:                              '1'
    base:                               '1'
    shift:                              '!'
}

# " -> @ in shift
key 2 {
    label:                              '2'
    base:                               '2'
    shift:                              '"'
}

key 3 {
    label:                              '3'
    base:                               '3'
    shift:                              '#'
}

key 4 {
    label:                              '4'
    base:                               '4'
    shift:                              '$'
}

key 5 {
    label:                              '5'
    base:                               '5'
    shift:                              '%'
}

# ^ -> & in shift
key 6 {
    label:                              '6'
    base:                               '6'
    shift:                              '&'
    
}

# & -> ' in shift
key 7 {
    label:                              '7'
    base:                               '7'
    shift:                              '\''
}

# * -> ( in shift
key 8 {
    label:                              '8'
    base:                               '8'
    shift:                              '('
}

# ( -> ) in shift
key 9 {
    label:                              '9'
    base:                               '9'
    shift:                              ')'
}

# ) -> none in shift
key 0 {
    label:                              '0'
    base:                               '0'
    shift:                              none
}

# _ -> = in shift
key MINUS {
    label:                              '-'
    base:                               '-'
    shift:                              '='
}

# = -> ^ in base
# + -> ~ in shift
key EQUALS {
    label:                              '^'
    base:                               '^'
    shift:                              '~'
}

# added
# YEN is defined in KeycodeLabels.h
key YEN {
    label:                              '\u00a5'
    base:                               '\u00a5'
    shift:                              '|'
}

### ROW 2

key Q {
    label:                              'Q'
    base:                               'q'
    shift, capslock:                    'Q'
}

key W {
    label:                              'W'
    base:                               'w'
    shift, capslock:                    'W'
}

key E {
    label:                              'E'
    base:                               'e'
    shift, capslock:                    'E'
}

key R {
    label:                              'R'
    base:                               'r'
    shift, capslock:                    'R'
}

key T {
    label:                              'T'
    base:                               't'
    shift, capslock:                    'T'
}

key Y {
    label:                              'Y'
    base:                               'y'
    shift, capslock:                    'Y'
}

key U {
    label:                              'U'
    base:                               'u'
    shift, capslock:                    'U'
}

key I {
    label:                              'I'
    base:                               'i'
    shift, capslock:                    'I'
}

key O {
    label:                              'O'
    base:                               'o'
    shift, capslock:                    'O'
}

key P {
    label:                              'P'
    base:                               'p'
    shift, capslock:                    'P'
}

# added
key AT {
    label:                              '@'
    base:                               '@'
    shift:                              '`'
}

# replace key code
key LEFT_BRACKET {
    label:                              '['
    base:                               '['
    shift:                              '{'
}

# moved RIGHT_BRACKET to ROW 3

# removed BACKSLASH 

### ROW 3

key A {
    label:                              'A'
    base:                               'a'
    shift, capslock:                    'A'
}

key S {
    label:                              'S'
    base:                               's'
    shift, capslock:                    'S'
}

key D {
    label:                              'D'
    base:                               'd'
    shift, capslock:                    'D'
}

key F {
    label:                              'F'
    base:                               'f'
    shift, capslock:                    'F'
}

key G {
    label:                              'G'
    base:                               'g'
    shift, capslock:                    'G'
}

key H {
    label:                              'H'
