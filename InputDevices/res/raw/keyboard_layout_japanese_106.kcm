# : -> + in shift
key SEMICOLON {
    label:                              ';'
    base:                               ';'
    shift:                              '+'
}

# ' -> : in base
# " -> * in shift
key APOSTROPHE {
    label:                              ':'
    base:                               ':'
    shift:                              '*'
}

# move from ROW 2
# replace key code
key RIGHT_BRACKET {
    label:                              ']'
    base:                               ']'
    shift:                              '}'
}

