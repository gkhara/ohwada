			android:layout_width="0dp"
    		android:layout_height="wrap_content"
    		android:layout_weight="2"
			android:text="@string/button_prev"
			/>
		<EditText
			android:id="@+id/edit_any"
			android:hint="@string/edit_any"
			android:layout_width="0dp"
    		android:layout_height="wrap_content"
    		android:layout_weight="3"
